{
  "version": 2,
  "builds": [
    {
      "src": "myrental/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myrental/wsgi.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.9",
      "memory": 1024
    }
  }
}

{
  "version": 2,
  "builds": [
      {
          "src": "myrental/manage.py",
          "use": "@vercel/python",
          "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.9"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "myrental/manage.py"
      }
  ]
}
